{
  "name": "yandex-alice-smart-home",
  "version": "1.0.0",
  "description": "Yandex Alice Smart Home API",
  "main": "dist/server.js",
  "private": true,
  "license": "ISC",
  "scripts": {
    "prepare": "husky install",
    "build": "npx tsc",
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "NODE_ENV=development nodemon src/server.ts",
    "deploy:docker": "./scripts/deploy-docker.sh",
    "format": "prettier --check --ignore-path .gitignore .",
    "lint": "eslint . --ext js,ts --quiet",
    "typecheck": "tsc --noEmit --project ./tsconfig.json --emitDeclarationOnly false",
    "test": "exit 0"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "object-hash": "^3.0.0",
    "proxmox-api": "^1.0.0",
    "ramda": "^0.28.0",
    "uuid-by-string": "^4.0.0",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/object-hash": "^3.0.2",
    "@types/ramda": "^0.28.22",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-turisap": "^0.0.18",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "packageManager": "yarn@3.3.1"
}
